public class Main {
    public static void main(String[] args) {
        B b = new B();
        b.display();
        b.display1();
    }
}
class A {
    int x = 10;
    void display() {
        System.out.println("Value of x: " + x+"\n");
    }
}

class B extends A {
    int y = 20;
    void display1() {
        System.out.println("Value of x: "  + x  );
        System.out.println("Value of y: " + y);
    }
}
